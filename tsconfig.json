{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "declaration": true,
    "target": "es5",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "noErrorTruncation": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
