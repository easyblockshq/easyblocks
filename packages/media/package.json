{
  "name": "@easyblocks/media",
  "version": "0.4.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "react-server": {
        "browser": "./dist/es/index.js",
        "node": "./dist/cjs/index.react-server.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "browser": "./dist/es/index.js",
        "node": "./dist/cjs/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/",
    "dev": "concurrently -n bundle,emit-declarations 'npm run bundle:watch' 'npm run emit-declarations:watch'",
    "build": "npm run bundle && npm run emit-declarations",
    "bundle": "NODE_ENV=production rollup -c rollup.config.js",
    "bundle:watch": "NODE_ENV=development  rollup -c rollup.config.js --watch",
    "emit-declarations": "tsc",
    "emit-declarations:watch": "npm run emit-declarations -- --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@easyblocks/core": "^0.4.0",
    "@easyblocks/design-system": "workspace:*",
    "@easyblocks/editor": "workspace:*",
    "styled-components": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "devDependencies": {
    "@easyblocks/build-tools": "^0.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26"
  },
  "files": [
    "dist/",
    "package.json",
    "LICENSE.md"
  ]
}
