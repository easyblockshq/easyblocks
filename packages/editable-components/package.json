{
  "name": "@easyblocks/editable-components",
  "version": "0.2.3",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/es/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "dev": "concurrently -n bundle,emit-declarations 'npm run bundle:watch' 'tsc --watch --preserveWatchOutput'",
    "build": "npm run bundle && tsc",
    "bundle": "NODE_ENV=production rollup -c rollup.config.js",
    "bundle:watch": "NODE_ENV=development rollup -c rollup.config.js --watch",
    "check": "tsc && eslint --quiet src/"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@easyblocks/core": "workspace:*",
    "@vimeo/player": "^2.16.4",
    "fast-xml-parser": "^4.1.3",
    "lodash": "^4.17.21",
    "postcss-value-parser": "^3.3.0",
    "react-textarea-autosize": ">=7.1",
    "slate": "0.77.2",
    "slate-react": "0.77.2",
    "type-fest": "^3.13.1"
  },
  "devDependencies": {
    "@easyblocks/app-utils": "^0.1.0",
    "@easyblocks/build-tools": "^0.1.0",
    "@easyblocks/reduce-css-calc": "workspace:*",
    "@easyblocks/utils": "workspace:*",
    "@jest/types": "^26.6.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/vimeo__player": "^2.18.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0"
  },
  "files": [
    "dist/",
    "package.json",
    "LICENSE.md"
  ]
}
