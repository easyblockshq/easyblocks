{
  "name": "@easyblocks/editor",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "browser": "./dist/es/index.js",
        "node": "./dist/cjs/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/",
    "dev": "concurrently -n bundle,emit-declarations 'npm run bundle:watch' 'npm run emit-declarations:watch'",
    "build": "npm run bundle && npm run emit-declarations",
    "bundle": "NODE_ENV=production rollup -c rollup.config.js",
    "bundle:watch": "NODE_ENV=development rollup -c rollup.config.js --watch",
    "emit-declarations": "tsc",
    "emit-declarations:watch": "tsc --watch --preserveWatchOutput",
    "test": "jest",
    "test:debug": "NODE_OPTIONS='--inspect-brk' jest --runInBand",
    "check": "tsc && eslint --quiet src/"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@easyblocks/core": "workspace:*",
    "@easyblocks/editable-components": "workspace:*",
    "@easyblocks/design-system": "workspace:*",
    "@emotion/is-prop-valid": "^1.2.1",
    "@popperjs/core": "^2.11.5",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@react-aria/tooltip": "^3.1.8",
    "@tanstack/react-query": "^4.29.12",
    "color-string": "^1.5.3",
    "contentful": "^9.1.4",
    "crypto-js": "^4.1.1",
    "final-form": "^4.20.4",
    "final-form-arrays": "^3.0.1",
    "lodash": "^4.17.21",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.17.3",
    "react-dismissible": "^1.3.0",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^10.2.2",
    "react-final-form": "^6.5.7",
    "react-modal": "^3.12.1",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.10.0",
    "styled-components": "^6.0.7",
    "type-fest": "^3.11.1",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "@easyblocks/build-tools": "^1.5.2",
    "@easyblocks/app-utils": "^1.5.2",
    "@easyblocks/compiler": "^1.5.2",
    "@easyblocks/utils": "workspace:*",
    "@easyblocks/test-utils": "^1.5.2",
    "@easyblocks/reduce-css-calc": "workspace:*",
    "@jest/types": "^26.6.2",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/color-string": "^1.5.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.192",
    "@types/react": "^18.2.20",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.13.1",
    "babel-plugin-styled-components": "^2.0.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "version": "1.5.2"
}
